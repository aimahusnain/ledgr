generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Order {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  date   DateTime
  amount Float
}

model Payout {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  date            DateTime
  initialBalance  Float
  payout          Float
  withdrawals     Float
}

model Configuration {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  commission Float
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  username  String   @unique
  email     String   @unique
  picture   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}